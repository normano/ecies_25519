{
  "name": "@excsn/ecies_25519",
  "description": "Elliptic Curve Integrated Encryption Scheme using x25519",
  "version": "1.0.0",
  "author": "Norman O <normo.pub@gmail.com>",
  "license": "MPL-2.0",
  "main": "dist/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "APP_ENV=test yarn run ava --tap",
    "test:cov": "yarn run nyc -r cobertura -r html -r text yarn run test",
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/normano/ecies_25519.git"
  },
  "dependencies": {
    "@stablelib/x25519": "^1.0.1",
    "asn1.js": "^5.4.1",
    "fast-sha256": "^1.3.0",
    "fs-extra": "^10.0.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^16.11.7",
    "ava": "^3.15.0",
    "nyc": "^15.1.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "failFast": true,
    "tap": true,
    "powerAssert": false,
    "files": [
      "test/**/*",
      "!test/src/**"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
